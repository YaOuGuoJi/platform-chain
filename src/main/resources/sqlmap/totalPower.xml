<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bester.platform.platformchain.dao.TotalPowerDao">

    <!--直接写SQL-->

    <insert id="insertTotalPower" parameterType="integer">
        INSERT INTO
        total_power(day,TotalPower,AddTime,UpdateTime)
        VALUES
        (
        #{day},
        #{totalPower},
        NOW(),
        NOW())
    </insert>

</mapper>
